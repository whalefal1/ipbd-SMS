<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ipbd.ipbdsms.mapper.TbLoginMapper">

  <select id="selectPasswordByUsername" parameterType="string" resultType="com.ipbd.ipbdsms.entity.TbLogin">
    select password,role from tb_login where username = ${username}
  </select>
</mapper>
